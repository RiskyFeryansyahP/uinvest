# GraphQL schema example
#
# https://gqlgen.com/getting-started/


type User {
  id: ID!
  firstname: String!
  lastname: String!
  email: String!
  phonenumber: String!
  password: String!
}

input LoginInput {
  email: String!
  password: String!
}

input RegisterInput {
  firstname: String!
  lastname: String!
  email: String!
  phonenumber: String!
  password: String!
}

type Query {
  login(input: LoginInput!): User!
  register(input: RegisterInput!): User!
}